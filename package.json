		"test": "vitest"
	},
	"dependencies": {
		"@opennextjs/cloudflare": "^1.2.1",
		"next": "15.3.3",
		"react": "^19.0.0",
		"react-dom": "^19.0.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/jest-dom": "^6.4.1",
		"@testing-library/react": "^16.3.0",
		"@types/node": "^20.19.0",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"eslint": "^9",
		"eslint-config-next": "15.3.3",
		"jsdom": "^26.1.0",
		"tailwindcss": "^4",
		"typescript": "^5",
		"vitest": "^3.2.3",
		"wrangler": "^4.19.1"
